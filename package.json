  "devDependencies": {
    "@parcel/transformer-sass": "2.12.0",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "posthtml-include": "^2.0.1",
    "sass": "^1.77.8"
  },
  "dependencies": {
    "swiper": "^11.1.12"
  }
}